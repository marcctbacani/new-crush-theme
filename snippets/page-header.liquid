    {% if current_tags %}<a href="{{ blog.url }}">{% endif %}
    {% comment %}ly_code_replace_for_[ blog.title ]_begin{% endcomment %}{% include 'ly-title' with blog %}{{ ly_translation }}{% comment %}ly_code_replace_end{% endcomment %}
    {% if current_tags %}</a>{% endif %}
  </h1>

  {% if settings.blog-show-rss-icon %}
    <p class="blog-subscribe"><a href="{{ blog.url }}.atom">{% comment %}ly_i18n_replace_for_[ 'blogs.blog.subscribe' | t ]_begin{% endcomment %}{% capture ly_template %}{{ 'blogs.blog.subscribe' | t }}{% endcapture %}{% include 'ly-static-string' with '9425183' %}{% if ly_translation %}{% assign escape_content = true %}{% assign ly_template = ly_translation %}{% else %}{% assign escape_content = false %}{% endif %}{% if escape_content %}{{ ly_template | escape}}{% else %}{{ ly_template }}{% endif %}{% comment %}ly_i18n_replace_end{% endcomment %}</a></p>
  {% endif %}
